{
    "env": {
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "indent": ["warn", 4],
        "no-console": "warn",
        "object-curly-newline": ["error", {
            // "ObjectExpression": "always",
            "ObjectPattern": { "multiline": false }
          }
        ],
        "no-param-reassign": "off",
        "max-len": ["warn", 120]
    }
}
